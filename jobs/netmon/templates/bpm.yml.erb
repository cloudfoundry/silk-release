---
processes:
  - name: netmon
    executable: "/var/vcap/jobs/netmon/bin/start"
    additional_volumes:
    - path: /var/vcap/data/container-metadata
      writable: true
    - path: /var/vcap/data/garden-cni
      writable: true
    capabilities:
    - NET_RAW
    - NET_ADMIN
    unsafe:
      privileged: true
